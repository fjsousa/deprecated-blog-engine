include ./header

body
  include ./navigation

  div#container
    div#content
      yield
      #disqus_thread
        |     
        script(type='text/javascript').
          /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
          var disqus_shortname = 'myblogabc123'; // required: replace example with your forum shortname
          /* * * DON'T EDIT BELOW THIS LINE * * */
          (function() {
          var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
          dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
          (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
          })();
        |     
        noscript
          | Please enable JavaScript to view the 
          a(href='https://disqus.com/?ref_noscript') comments powered by Disqus.  	

  #footer

  script(type='text/javascript', src='http://latex.codecogs.com/latexit.js')
  script(type='text/javascript', src='//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js')
  script.
    hljs.initHighlightingOnLoad();


